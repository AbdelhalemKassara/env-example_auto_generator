{
  "name": "env-example-auto-generator",
  "displayName": "env-example_auto_generator",
  "description": "auto prompts the user to create an .env-example when an .env file is detected   and keeps it in sync",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "env-example-auto-generator.helloWorld",
        "title": "test Command",
        "_random_string_I_made_up_to_write_comments": " the command string above can be anything you want. (I assume you can't override existing commands)"
      },
      {
        "command": "env-example-auto-generator.curtime",
        "title": "Current Time"
      },
      {
        "command": "env-example-auto-generator.envFileOpen",
        "title": "Env File Open",
        "when": "editorLangId == 'typescript'"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "testOriginal": "vscode-test",
    "test": "jest /out --watchAll --verbose"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "18.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0",
    "jest": "^29.7.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "vscode-uri": "^3.0.8"
  }
}
